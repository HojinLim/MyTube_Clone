{
  "kind": "collectionType",
  "collectionName": "youtube_medias",
  "info": {
    "name": "youtube",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "contents": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "isPublic": {
      "type": "boolean",
      "default": false
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "createdBy": {
      "type": "string"
    },
    "duration": {
      "type": "string"
    },
    "sort": {
      "type": "enumeration",
      "enum": [
        "movie",
        "anime",
        "music",
        "game"
      ]
    },
    "views": {
      "type": "integer"
    },
    "comment": {
      "model": "comments",
      "via": "youtube_medias"
    },
    "sub_users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "subscribes"
    },
    "dislike_user": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "dislike_youtube"
    },
    "like_user": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "like_youtube"
    },
    "later_users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "youtubes",
      "dominant": true
    },
    "created_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "created_youtubes"
    }
  }
}
