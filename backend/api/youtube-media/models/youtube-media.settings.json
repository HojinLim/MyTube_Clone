{
  "kind": "collectionType",
  "collectionName": "youtube_medias",
  "info": {
    "name": "youtube-media",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "contents": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "isPublic": {
      "type": "boolean",
      "default": false
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "createdBy": {
      "type": "string"
    },
    "duration": {
      "type": "string"
    },
    "sort": {
      "type": "enumeration",
      "enum": [
        "movie",
        "anime",
        "music",
        "game"
      ]
    },
    "views": {
      "type": "integer"
    }
  }
}
